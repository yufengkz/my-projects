        // ‰∏çÂê´002
        data.mapUserInfo.push({
          role: '002',
          keyId: 2,
          cname: 'ÈáëËûç‰∏ìÂëò',
          name: '',
          mobile: '',
          email: ''
        })
      }
      // ÊéíÂ∫è ‰ªéÂ§ßÂà∞Â∞è 003 002 001
